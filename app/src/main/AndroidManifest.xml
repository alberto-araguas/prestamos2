<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aaraguas.ilernaprestamos">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.prestamostheme">
        <activity
            android:name=".FamiliaActivity"
            android:exported="false" />
        <activity
            android:name=".ProveedoresActivity"
            android:exported="false" />
        <activity
            android:name=".LugaresActivity"
            android:exported="false" />
        <activity
            android:name=".UsuariosActivity"
            android:exported="false" />
        <activity android:name="com.aaraguas.ilernaprestamos.SplashscreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.aaraguas.ilernaprestamos.AgregarActivity" />
        <activity android:name="com.aaraguas.ilernaprestamos.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>